{
  "name": "ngx-highlightjs-demo",
  "version": "0.0.0",
  "description": "Instant code highlighting, auto-detect language, super easy to use.",
  "homepage": "http://github.com/murhafsousli/ngx-highlightjs",
  "author": {
    "name": "Murhaf Sousli",
    "url": "http://github.com/murhafsousli"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/murhafsousli/ngx-highlightjs.git"
  },
  "bugs": {
    "url": "http://github.com/murhafsousli/ngx-highlightjs/issues"
  },
  "keywords": [
    "angular",
    "highlight",
    "highlightjs",
    "prism",
    "syntax",
    "code"
  ],
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve ngx-highlightjs-demo",
    "build-demo": "ng build ngx-highlightjs-demo --configuration production",
    "build-lib": "ng build ngx-highlightjs --configuration production",
    "test-lib": "ng test ngx-highlightjs",
    "test-lib-headless": "ng test ngx-highlightjs --watch=false --no-progress --browsers=ChromeHeadless",
    "link-lib": "npm link dist/ngx-highlightjs && npm link ngx-highlightjs",
    "publish-lib": "npm publish dist/ngx-highlightjs",
    "e2e": "ng e2e",
    "dev:ssr": "ng run ngx-highlightjs-demo:serve-ssr",
    "serve:ssr": "node dist/ngx-highlightjs-demo/server/main.js",
    "build:ssr": "ng build ngx-highlightjs-demo && ng run ngx-highlightjs-demo:server",
    "prerender": "ng run ngx-highlightjs-demo:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.1.2",
    "@angular/cdk": "^16.1.2",
    "@angular/common": "^16.1.2",
    "@angular/compiler": "^16.1.2",
    "@angular/core": "^16.1.2",
    "@angular/flex-layout": "^15.0.0-beta.42",
    "@angular/forms": "^16.1.2",
    "@angular/material": "^16.1.2",
    "@angular/platform-browser": "^16.1.2",
    "@angular/platform-browser-dynamic": "^16.1.2",
    "@angular/platform-server": "^16.1.2",
    "@angular/router": "^16.1.2",
    "@nguniversal/express-engine": "^16.1.0",
    "express": "^4.15.2",
    "highlight.js": "^11.8.0",
    "highlightjs-line-numbers.js": "^2.8.0",
    "ngx-scrollbar": "^13.0.1",
    "rxjs": "^7.5.1",
    "tslib": "^2.3.1",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.1",
    "@angular-devkit/core": "^16.1.1",
    "@angular-devkit/schematics": "^16.1.1",
    "@angular/cli": "^16.1.1",
    "@angular/compiler-cli": "^16.1.2",
    "@angular/language-service": "^16.1.2",
    "@nguniversal/builders": "^16.1.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~4.0.0",
    "@types/node": "^14.15.0",
    "jasmine-core": "~4.1.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^16.1.0",
    "typescript": "~5.0.4"
  }
}